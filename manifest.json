{
    "manifest_version": 2,
    "name": "SharePoint navigator",
    "version": "1.0",
    "description": "Easy and quick navigation through SharePoint sites for administrators",
    
    "background": {
        "persistent": false,
        "scripts": [
            "app.js"
        ]
    },
    "permissions": [
        "http://*.sharepoint.com/*",
        "https://*.sharepoint.com/*",
        "tabs"
    ],
    "page_action": {
        "default_title": "SharePoint navigator",
        
        "default_popup": "popup.html"
    }
}